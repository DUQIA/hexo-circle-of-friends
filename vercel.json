{
    "version": 2,
    "public": true,
    "build": {
      "env": {
        "HOME": "/root"
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/vercel.py"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "*"
          }
        ]
      }
    ]
  }
